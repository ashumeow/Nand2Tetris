/**
 * 4-way de-multiplexer
 * {a,b,c,d} = {in,0,0,0} if sel == 00
 *             {0,in,0,0} if sel == 01
 *             {0,0,in,0} if sel == 10
 *             {0,0,0,in} if sel == 11
 */


CHIP DMux4Way {
// chip interface
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    // chip implementation
    DMux(in = in, sel = sel[1], a = oab, b = ocd);
    DMux(in = oab, sel = sel[0], a = a, b = b);
    DMux(in = ocd, sel = sel[0], a = c, b = d);
}
